<Window x:Class="MetricsManagerClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MetricsManagerClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="640" Background="Aqua">
    <Grid>
        <StackPanel>
            <local:CpuMaterialCards Grid.Column="0" Grid.Row="0" x:Name="CpuChart" Loaded="CpuChart_Loaded"></local:CpuMaterialCards>
            <local:RamMaterialCards Grid.Column="1" Grid.Row="0" x:Name="RamChart" Loaded="RamChart_Loaded"></local:RamMaterialCards>
        </StackPanel>
    </Grid>
</Window>
